<div class="container">

	<table class="table table-stripe">
		<thead>
			<th>NAME</th>
			<th>ADDRESS</th>
			<th>PHONE NUMBER</th>
		</thead>
		<tbody>
			<tr ng-repeat="user in con1.attributes">
